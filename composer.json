{
    "name": "sidredin/docker_php8.0_mysql8.0_env",
    "type": "project",
    "description": "Docker environment with PHP 8.0, MySQL 8.0, Nginx and some other services",
    "keywords": [
        "php",
        "mysql",
        "docker"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
